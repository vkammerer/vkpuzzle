{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "new-cap": 0,
    "no-console": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "indent": [2, "tab", {"SwitchCase": 1}],
    "comma-dangle": 0,
    "react/prop-types": 0,
    "react/jsx-indent-props": [2, "tab"]
  }
}
